<section class="status">
    <div class="container mx-auto">
        <div class="w-2/3 mx-auto status-box pt-8">
            <div class="flex items-center justify-between mb-12">
                <h1 class="text-xl font-bold">Track Delivery Status</h1>
                <h6 class="bg-white py-1 rounded-full px-4 text-green-600 text-sm"> <%=  result._id %> </h6>
                <input type="hidden" id="hiddeninput" value="<%= JSON.stringify(result)  %>">
            </div>
            <ul>
                <li class="status_line text-xl pb-16 " data-status="order_placed"> <span>Order Placed</span></li>
                <li class="status_line text-xl pb-16 " data-status="confirmed"> <span>Order confirmation</span></li>
                <li class="status_line text-xl pb-16" data-status="prepared"> <span>Preparation</span></li>
                <li class="status_line text-xl pb-16" data-status="deliverd"> <span>Out for delivery</span></li>
                <li class="status_line text-xl " data-status="completed"> <span>Complete</span></li>
            </ul>
        </div>
    </div>
</section>